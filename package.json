{
    "name": "boilerplate-react",
    "version": "1.0.0",
    "author": "Calvin Tiley",
    "description": "A boilerplate for React",
    "main": "index.js",
    "private": true,
    "scripts": {
        "build": "webpack --config webpack.production.js",
        "build-dev": "webpack --config webpack.development.js",
        "lint": "eslint \"*/**/*.{js,ts,tsx}\" --max-warnings 0",
        "generate": "plop",
        "start": "webpack serve --config webpack.development.js",
        "setup": "npm ci || npm install",
        "test": "jest --coverage",
        "test:dev": "jest --watch --verbose=true --coverage",
        "test:unit": "jest unit",
        "test:integration": "jest integration"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Epiksha/boilerplate-react.git"
    },
    "keywords": [
        "React",
        "Boilerplate",
        "Sass",
        "Webpack",
        "Babel",
        "Jest",
        "ESLint"
    ],
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Epiksha/boilerplate-react/issues"
    },
    "homepage": "https://github.com/Epiksha/boilerplate-react#readme",
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest-setup.ts"
        ],
        "moduleNameMapper": {
            "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/mocks/fileMock.js"
        },
        "testEnvironment": "jsdom"
    },
    "resolutions": {
        "webpack": "^5.68.0"
    },
    "devDependencies": {
        "@babel/cli": "7.20.7",
        "@babel/core": "7.20.12",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@commitlint/cli": "17.4.2",
        "@commitlint/config-conventional": "17.4.2",
        "@fullhuman/postcss-purgecss": "5.0.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@types/jest": "^29.2.5",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@types/react-router-dom": "5.3.3",
        "@types/uuid": "^9.0.0",
        "@types/webpack": "5.28.0",
        "@types/webpack-env": "1.18.0",
        "@typescript-eslint/eslint-plugin": "5.48.1",
        "@typescript-eslint/parser": "5.48.1",
        "babel-loader": "9.1.2",
        "babel-plugin-transform-require-context": "0.1.1",
        "css-loader": "6.7.3",
        "css-minimizer-webpack-plugin": "4.2.2",
        "cssnano": "5.1.14",
        "eslint": "8.31.0",
        "eslint-plugin-react": "7.32.0",
        "eslint-plugin-storybook": "^0.6.10",
        "eslint-webpack-plugin": "3.2.0",
        "html-loader": "4.2.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "8.0.3",
        "jest": "29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "lint-staged": "13.1.0",
        "plop": "3.1.1",
        "postcss": "8.4.21",
        "postcss-import": "15.1.0",
        "postcss-loader": "7.0.2",
        "postcss-preset-env": "7.8.3",
        "sass": "1.57.1",
        "sass-loader": "13.2.0",
        "sass-utilizer": "3.2.7",
        "style-loader": "3.3.1",
        "svg-sprite-loader": "6.0.11",
        "svgo": "3.0.2",
        "svgo-loader": "3.0.3",
        "ts-loader": "9.4.2",
        "typescript": "4.9.4",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "4.11.1",
        "webpack-merge": "5.8.0"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hot-loader": "4.13.1",
        "react-router-dom": "6.6.2",
        "uuid": "^9.0.0"
    }
}
